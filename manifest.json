{
  "manifest_version": 2,

  "name": "Open-as",
  "description": "Open ids you find on webpages on other apis",
  "version": "1.0",

  "permissions": [
    "contextMenus",
    "storage"
  ],
  "sandbox": {
     "pages": ["sandbox.html"]
  },
  "background": {
    "page": "bridge.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "config.html"
  }
}
